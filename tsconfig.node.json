{
  "extends": "./node_modules/@tsconfig/node18/tsconfig.json",
  "include": [
    "vite.config.*",
    "vitest.config.*",
    "cypress.config.*",
    "nightwatch.conf.*",
    "playwright.config.*"
  ],
  "compilerOptions": {
    "composite": true,
    "module": "ESNext",
    "types": ["node"],
        // typescript can detect Modules in dynamic path
        "moduleResolution": "Node"
  }
}
